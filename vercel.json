{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/notion.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/notion", "dest": "/api/notion.js" },
    { "src": "/(.*)\\.(png|jpg|jpeg|gif|svg)$", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
